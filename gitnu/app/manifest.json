{
  "name": "Gitnu - Git interactive terminal not unix",
  "description": "An interative terminal for Git in Chrome",
  "version": "1.0",
  "manifest_version": 2,
  "permissions": [
    "https://www.google-analytics.com/",
    "https://github.com/*",
    "clipboardRead",
    "clipboardWrite",
    {
      "fileSystem": [
        "write",
        "retainEntries",
        "directory"
      ]
    },
    {
      "socket": [
        "tcp-connect",
        "tcp-listen"
      ]
    },
    "storage",
    "webview",
    "contextMenus"
  ],
  "app": {
    "background": {
      "scripts": ["background.js"]
    }
  },
  "icons": {"128": "assets/g-128.png", "16": "assets/g-16.png"}
}

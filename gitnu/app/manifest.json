{
  "name": "Gitnu - Git interactive terminal not unix",
  "description": "An interative terminal for Git in Chrome",
  "version": "0.1",
  "manifest_version": 2,
  "permissions": [
    "storage",
    {"fileSystem": ["write", "retainEntries", "directory"]}
  ],
  "app": {
    "background": {
      "scripts": ["background.js"]
    }
  },
  "icons": {"128": "dart_icon.png"}
}
